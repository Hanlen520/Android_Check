{
    "--apk":"apk_path",
    "--output":"output_path",
    "--format":"mm.html,mm.json",

    "--formatConfig":[
        {
            "name":"-countMethod",
            "group":[
                {
                    "name":"Android System",
                    "package":"android"
                },
                {
                    "name":"java system",
                    "package":"java"
                },
                {
                    "name":"com.test",
                    "package":"com.test"
                }
            ]
        }
    ],

    "options":[
        {
            "name":"-manifest"
        },
        {
            "--suffix":"png, jpg, jpeg, gif, arsc",
